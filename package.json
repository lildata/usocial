  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.2",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "http-server": "^0.11.1",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
